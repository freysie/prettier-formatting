{
  "name": "prettier-formatting",
  "version": "2.0.0",
  "description": "Format source code on Apple platforms using JavaScriptCore and Prettier.",
  "author": "Freya Alminde <72786+freysie@users.noreply.github.com>",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "prettier": "^3.6.2",
    "prettier-plugin-sql": "0.19.2",
    "@prettier/sync": "^0.6.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.8"
  },
  "scripts": {
    "build:sql-plugin": "esbuild sql-plugin-standalone.entry.js --bundle --format=iife --global-name=PrettierSqlPluginStandalone --platform=browser --external:prettier --outfile=sql-plugin-standalone.js",
    "build:prettier-sync": "esbuild prettier-sync.entry.js --bundle --format=iife --global-name=PrettierSyncBundle --platform=browser --outfile=prettier-sync.js"
  }
}
